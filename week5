function solution(money) {
    let answer = [];
    let answer1 = parseInt(money / 5500); // 금액 나누기 5500 + 정수형변환
    let answer2 = money - (5500*answer1); // 위에서 구한 금액에서 5500을 나눈값과 금액의 차
    answer = [answer1, answer2]
    return answer;
}
