function solution(n) {
     let answer = 2;
        const number = Math.sqrt(n); // n의 제곱근을 number로 지정
        if(parseInt(number)*parseInt(number)==n && number>0){
					// 제곱근 number는 자연수가 되어야하며
					// n을 정수로 형변환 한 값의 곱은 n이 되어야한다. 
            answer = 1;
        }
        console.log(number, n)
    return answer;
}
